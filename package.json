{
    "name": "@nodesoft/lambdi",
    "version": "1.0.0",
    "description": "Lambda with Typescript and DI",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "build": "tsc --build tsconfig.build.json && cp package.json build/ && cp README.md build/",
        "clean": "rm -r build/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nodesoft-io/lambdi.git"
    },
    "author": "Juneil",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nodesoft-io/lambdi/issues"
    },
    "homepage": "https://github.com/nodesoft-io/lambdi#readme",
    "jest": {
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "rootDir": "./",
        "roots": [
            "./test"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "testMatch": [
            "**/*.test.(ts|js)"
        ],
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.json"
            }
        }
    },
    "dependencies": {
        "aws-xray-sdk-core": "^3.5.0",
        "injection-js": "^2.4.0",
        "pino": "^8.14.1",
        "reflect-metadata": "^0.1.13",
        "ajv": "^8.12.0",
        "ajv-errors": "^3.0.0",
        "ajv-keywords": "^5.1.0"
    },
    "devDependencies": {
        "@types/ajv": "^1.0.0",
        "@types/ajv-errors": "^2.0.1",
        "@types/aws-lambda": "^8.10.119",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.2",
        "@types/pino": "^7.0.5",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "aws-sdk": "^2.1408.0",
        "eslint": "^8.43.0",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "peerDependencies": {
        "@types/aws-lambda": "^8.10.119",
        "aws-sdk": "^2.1408.0"
    }
}